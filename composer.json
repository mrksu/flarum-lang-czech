{
    "name": "mrksu/flarum-lang-czech",
    "type": "flarum-extension",
    "description": "Czech language pack for Flarum.",
    "homepage": "https://github.com/mrksu/flarum-lang-czech",
    "keywords": ["locale"],
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Czech language pack",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "cs",
            "title": "Czech"
        }
    }
}
